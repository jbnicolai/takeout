{
  "name": "takeout",
  "version": "0.1.0",
  "description": "Get the file contents, seamlessly available for both local file system and HTTP(S)",
  "repository": "shinnn/takeout",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "eslint --ignore-path .gitignore . & jscs .",
    "test": "node test/test.js | tap-spec",
    "coverage": "istanbul cover test/test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/takeout/blob/master/LICENSE"
    }
  ],
  "bin": "cli.js",
  "files": [
    "cli.js",
    "index.js",
    "LICENSE"
  ],
  "keywords": [
    "local",
    "read",
    "file",
    "http",
    "https",
    "request",
    "get",
    "response",
    "url",
    "buffer",
    "seamless",
    "body"
  ],
  "dependencies": {
    "chalk": "^0.5.1",
    "fs-extra": "^0.12.0",
    "got": "^1.2.2",
    "is-http": "^0.0.2",
    "minimist": "^1.1.0",
    "readfile-directory-index-fallback": "1.0.0"
  },
  "devDependencies": {
    "eslint": "^0.8.2",
    "istanbul": "^0.3.2",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.7.3",
    "nop": "^1.0.0",
    "rimraf": "^2.2.8",
    "tap-spec": "^1.0.1",
    "tape": "^3.0.1"
  }
}
